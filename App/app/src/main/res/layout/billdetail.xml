<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efefef">


<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/bill_detial_fline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <ImageView
            android:id="@+id/bill_favicon"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:paddingRight="12dp"
            android:src="@drawable/star48"
            android:layout_weight="1"
            android:layout_marginRight="15dp"
            android:scaleType="fitEnd" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_secline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/bill_detial_fline"
        android:layout_marginTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bill ID:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="bill_id"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_thrline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/bill_detail_secline"
        android:layout_marginTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Title:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_title"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:text="title"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_fourline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detail_thrline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bill Type:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_billtype"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="bill type"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_fifline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detail_fourline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sponsor:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_sponsor"
            android:layout_width="192dp"
            android:layout_height="wrap_content"
            android:text="sponsor"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detial_sixline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detail_fifline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Chamber:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_chamber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="chamber"
            android:textSize="20sp"
            android:layout_weight="1"
            android:layout_marginRight="24dp"
            android:gravity="right"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_sevenline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detial_sixline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Status:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="status"
            android:textSize="20sp"
            android:layout_weight="1"
            android:layout_marginRight="24dp"
            android:gravity="right"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_eightline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detail_sevenline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Introduced On:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_introduced_on"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="introduced on"
            android:textSize="20sp"
            android:layout_weight="1"
            android:layout_marginRight="24dp"
            android:gravity="right"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_nineline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@id/bill_detail_eightline"
        android:layout_marginTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Congress URL:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_congressUrl"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="congress url"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bill_detail_tenline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="16dp"
        android:layout_below="@id/bill_detail_nineline">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Version Status:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_versionstatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="version status"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/elevenline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/bill_detail_tenline"
        android:layout_marginTop="16dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bill URL:"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:paddingLeft="20dp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" "
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/bill_detail_billurl"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="bill url"
            android:textSize="20sp"
            android:gravity="right"
            android:layout_weight="1"
            android:layout_marginRight="24dp"/>

    </LinearLayout>


</RelativeLayout>
</ScrollView>